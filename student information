#include <iostream>
#include <vector>
using namespace std;
struct student{
    string name;
    int ID;
};
int main (){
    int n;
    cout<<"How many student data are you Store"<<endl;
    cin>>n;
    vector<student>students;
    students.resize(n);
    for(int i=0; i<n; i++){
        cout<<"Student Name is  "<<i+1<<":  ";
        cin>>students[i].name;
        cout<<"Student ID is "<<i+1<<":  ";
        cin>>students[i].ID;
    }
    cout<<"=================>Student All information<========"<<endl;
    for(int i=0; i<n; i++){
        cout<<"Student "<<i+1<<" Name is" <<students[i].name<<" and Student ID is "<<students[i].ID<<endl;
    }
    return 0;
}
