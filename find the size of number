#include <iostream>
#include <vector>
using namespace std;
int main(){
    int n; 
    cout<<"Enter your number "<<endl;
    cin>>n;
    vector<int>number;
    for(int i=0; i<n; i++){
        int storenumber;
        cin>>storenumber;
        number.push_back(storenumber);
    }
    cout<<"Your number are "<<endl;
    for(int i=0; i<number.size(); i++){
        cout<<number[i]<<" ";
    }
    return 0;
}
