#include <iostream>
using namespace std;

int main() {
    int row, col;
    cout << "Enter your Row And Column: ";
    cin >> row >> col;
    int array[10][10];
    cout << "Enter matrix elements";
    for(int i = 0; i < row; i++){
        for(int j = 0; j < col; j++){
        cin >> array[i][j];
        }
    }
    cout << "Matrix";
    for(int i = 0; i < row; i++){
        for(int j = 0; j < col; j++){
        cout << array[i][j] << " ";
        }
        cout << endl;
    }
    int max_number = array[0][0];
    for(int i = 0; i < row; i++){
        for(int j = 0; j < col; j++){
            if(array[i][j] > max_number){
         max_number = array[i][j];
        }
        }
    }
    int second_last = 0;
    for(int i = 0; i < row; i++){
        for(int j = 0; j < col; j++){
            if(array[i][j] > second_last && array[i][j] < max_number){
            second_last = array[i][j];
        }
        }
    }
    cout << "1st Largest Number " << max_number << endl;
    cout << "2nd Largest Number " << second_last << endl;
    return 0;
}
