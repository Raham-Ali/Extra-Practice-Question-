#include <iostream>
#include<vector>
#include<algorithm>
using namespace std;
int main()
{
	vector<int>number={45,23,67,12,1,5,2,0,7,16};
	sort(number.begin(),number.end());
	cout<<"Now we are Sorting The Number"<<endl;
	for(int i=0; i<number.size(); i++){
	    cout<<number[i]<<" ";
	}
	cout<<endl;
	return 0;
}
